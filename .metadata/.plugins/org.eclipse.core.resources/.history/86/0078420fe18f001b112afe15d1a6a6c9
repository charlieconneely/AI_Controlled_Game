package ie.gmit.sw.ai;

public class Chase implements Command {
	
	private GameModel model;
	private char enemyID;
	private BFS bfs;
	private int row, col;
	
	public Chase(GameModel model, char enemyID, int row, int col) {
		this.model = model;
		this.enemyID = enemyID;
		this.row = row;
		this.col = col;
		bfs = new BFS(model);
	}
		
	@Override
	public void execute() {		
		System.out.println("Chase");
	}
}
