package ie.gmit.sw.ai;

public class testingoutloopthing {
	
	public static void main(String[] args) {
		int row = 0;
		int col = 0;
		
		for (int r = -1; r < 2; r++) { // row
			if (r == 0) continue; 
			for (int c = -1; c < 2; c++) {
				if (c == 0) continue;
				System.out.println("Row:" + r + ", Col:" + c);
			}
		}
	}

}
